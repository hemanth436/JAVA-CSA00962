class armstrongnumber
{
    public static void main(String arg[])
    {
        int n=121,m=n;
        int res=0;
        while(n!=0)
        {
            int rem=n%10;
            res+=rem*rem*rem;
            n=n/10;
        }
        if(res==m)
        {
            System.out.println("it is a armstrong number");
        }
        else
        {
            System.out.println("it is not a armstrong number");
        }
    }
}
